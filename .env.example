# Environment variables for docker-compose
# Copy this file to .env and fill in your values

# Backend configuration
BACKEND_TAG=dev
ASPNETCORE_ENVIRONMENT=Development

# Frontend configuration
FRONTEND_TAG=dev

# Database configuration
POSTGRES_DB=lilith
POSTGRES_USER=lilith
POSTGRES_PASSWORD=your_secure_password_here
DB_CONNECTION_STRING=Host=postgres;Port=5432;Database=lilith;Username=lilith;Password=your_secure_password_here

# JWT configuration
JWT_SECRET_KEY=your_jwt_secret_key_here_minimum_32_characters
JWT_ISSUER=https://api.temges.com
JWT_AUDIENCE=https://temges.com
